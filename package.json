{
  "engines": {
    "npm": ">=8.15.0",
    "node": ">=16.17.0"
  },
  "targets": {
    "default": {
      "sourceMap": true,
      "includeNodeModules": [
        "handlebars",
        "uuid"
      ],
      "engines": {
        "browsers": " 50%"
      }
    }
  },
  "staticFiles": {
    "staticPath": [
      "files/"
    ]
  },
  "name": "messenger_v",
  "version": "4.0.0",
  "description": "Messenger by Veronika Zayarnaya",
  "source": "src/index.html",
  "private": true,
  "scripts": {
    "dev": "npx webpack serve --server-type http",
    "add-bundles": "git add dist/*",
    "build": "npx webpack",
    "start": "npx webpack && node server.js",
    "test": "mocha"
  },
  "pre-commit": [
    "build",
    "test"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "parcelCleanPaths": [
    "dist",
    ".cache"
  ],
  "dependencies": {
    "@babel/preset-typescript": "7.18.6",
    "@babel/register": "7.18.9",
    "express": "4.18.1",
    "express-handlebars": "6.0.6",
    "handlebars": "4.7.7",
    "lodash": "4.17.21",
    "stylelint-config-standard": "28.0.0",
    "stylelint-csstree-validator": "2.0.0",
    "uuid": "8.3.2",
    "xhr2": "0.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@babel/register": "7.18.9",
    "@types/chai": "4.3.3",
    "@types/mocha": "9.1.1",
    "@types/node": "18.7.15",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "chai": "4.3.6",
    "css-loader": "6.7.1",
    "eslint": "8.21.0",
    "eslint-config-airbnb": "19.0.4",
    "handlebars-loader": "1.7.2",
    "jsdom": "20.0.0",
    "jsdom-global": "3.0.2",
    "mocha": "10.0.0",
    "pre-commit": "1.2.2",
    "sass": "1.54.9",
    "sass-loader": "13.0.2",
    "style-loader": "3.3.1",
    "stylelint": "14.11.0",
    "stylelint-scss": "4.3.0",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.0"
  }
}
