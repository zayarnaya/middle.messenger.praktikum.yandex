{
  "engines": {
    "npm": "8.14.0",
    "node": "v16.16.0"
  },
  "targets": {
    "default": {
      "sourceMap": true,
      "includeNodeModules": [
        "handlebars",
        "uuid"
      ],
      "engines": {
