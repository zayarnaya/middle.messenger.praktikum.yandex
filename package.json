    "@inventory/parcel-plugin-handlebars": "1.0.0",
    "@parcel/babel-preset-env": "2.7.0",
    "express": "4.18.1",
    "express-handlebars": "6.0.6",
    "handlebars": "4.7.7",
    "lodash": "4.17.21",
    "uuid": "8.3.2",
    "xhr2": "0.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@babel/register": "7.18.9",
    "@parcel/transformer-sass": "2.6.2",
    "@types/chai": "4.3.3",
    "@types/mocha": "9.1.1",
    "@types/node": "18.7.15",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "chai": "4.3.6",
    "eslint": "8.21.0",
    "eslint-config-airbnb": "19.0.4",
    "mocha": "10.0.0",
    "parcel": "2.6.2",
    "parcel-plugin-clean-easy": "1.0.2",
    "parcel-plugin-static-files-copy": "2.6.0",
    "parcel-transformer-hbs": "1.0.4",
    "stylelint": "14.9.1",
